! Fuentes:
xterm*faceName: Dejavu Sans Mono Book
xterm*faceSize:	9
URxvt*font:     xft:Terminus:size=8

! Configuración local
XTerm*locale: 	true

! Shell entrada por defecto...
XTerm*loginshell: true

! Enable bell urgent
xterm*bellIsUrgent: true

! scrollback...
XTerm* savelines: 10000

! Doble click selección de URL...
XTerm*charClass: 33:48,36-47:48,58-59:48,61:48,63-64:48,95:48,126:48

! Geometra de ventana...
xterm*geometry: 80x20


! special
xterm*foreground:   #818181
xterm*background:   #1b1b1b
xterm*cursorColor:  #16a085

! black
xterm*color0:       #000000
xterm*color8:       #0d0d0d

! red
xterm*color1:       #111111
xterm*color9:       #181818

! green
xterm*color2:       #222222
xterm*color10:      #272727

! yellow
xterm*color3:       #333333
xterm*color11:      #393939

! blue
xterm*color4:       #444444
xterm*color12:      #484848

! magenta
xterm*color5:       #555555
xterm*color13:      #595959

! cyan
xterm*color6:       #16a085
xterm*color14:      #05b08f

! white
xterm*color7:       #777777
xterm*color15:      #818181
